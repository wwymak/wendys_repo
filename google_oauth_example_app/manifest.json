{
    "name": "Test with OAuth",
    "version": "0.0.3",
    "manifest_version": 2,
    
    "oauth2": {
        "client_id": "1997",
        "scopes": ["https://www.strava.com/oauth/authorize"]
    },
    "app": {
        "background": {
            "scripts": ["js/background.js"]
        }
    },
    "icons": {
        "128": "Bicycle-icon-128px.png"
    },
    "key": "",

    "permissions": [
        "identity",
        "<all_urls>",
        "http://*/*",
        "https://*/*",
        "chrome://favicon/"
    ]
}